
procedure main() return integer{
	
	var y:string[50];
	var x:char;
	var z:boolean;
	
	while(z)
	{	
		z = foo(5,3);
		
	}

	if(z)
	{
		x = 'a';
		if(false)
		{
			x='r';
		}
		x = 'y';
	}

	return 5;
}

procedure foo(x,y:integer) return boolean{
	procedure goo(a,s:charptr;x:boolean;y:integer) return integer
	{
		if (x)
		{
		 y= 233 * 12 + 9;
		}
		return 0;
	}
	var k:boolean;
	var zx:charptr;

	while(false)
	{
		k= true;
	}
	x = goo(zx,zx,k,y);
	return false; 
}
